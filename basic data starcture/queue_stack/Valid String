problem link: https://www.hackerrank.com/contests/assignment-03-a-basic-data-structures-a-batch-03/challenges/valid-parentheses-16

solution:
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int test;
    cin>> test;
    while (test--)
    {
        string ss; cin>> ss;
        int cnt_a=0;
        int cnt_b=0;
        for (int  i = 0; i < ss.length(); i++)
        {
            if(ss[i]=='A')
            {
                cnt_a++;
            }
            else cnt_b++;

        }
        if(cnt_a== cnt_b)
        {
            cout<<"YES"<<endl;
        }
        else cout<<"NO"<<endl;
        
    }
    
    return 0;
}
