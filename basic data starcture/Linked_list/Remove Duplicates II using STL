https://www.hackerrank.com/contests/mid-term-exam-a-basic-data-structures-a-batch-03/challenges/remove-duplicated-ii/submissions/code/1382135384


solution

#include<bits/stdc++.h>
using namespace std;
int main()
{
    list<int>mylist;
    while (true)
    {
        int val; cin>> val;
        if(val== -1)
        {
            break;
        }
        mylist.push_back(val);
    }
    mylist.sort();
    mylist.unique();
    for(int x: mylist)
    {
        cout<<x<<" ";
    }
    
    return 0;
}
